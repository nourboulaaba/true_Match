<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllerConge.HistoriqueController">
    <TextField fx:id="idEmployeField" promptText="Entrez votre ID Employé" />
    <Button fx:id="rechercherButton" text="Rechercher" onAction="#rechercherHistorique" />
    <Label fx:id="errorLabel" textFill="red" />
    <TableView fx:id="tableView">
        <columns>
            <TableColumn fx:id="colId" text="ID Congé" />
            <TableColumn fx:id="colType" text="Type" />
            <TableColumn fx:id="colDebut" text="Début" />
            <TableColumn fx:id="colFin" text="Fin" />
            <TableColumn fx:id="colStatut" text="Statut" />
        </columns>
    </TableView>
</VBox>